<%= form_with model: @todo, local: true, url: user_todo_path, method: :patch do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <%= form.label :description %>
  <%= form.text_field :description %>
  <%= form.label :completed %>
  <%= form.check_box :completion_status %>
  <%= form.submit %>
<% end %>

